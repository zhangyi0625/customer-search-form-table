#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangyi/Desktop/rstack/rslib-project/rslib-template/node_modules/.pnpm/@rsbuild+core@1.4.9/node_modules/@rsbuild/core/bin/node_modules:/Users/zhangyi/Desktop/rstack/rslib-project/rslib-template/node_modules/.pnpm/@rsbuild+core@1.4.9/node_modules/@rsbuild/core/node_modules:/Users/zhangyi/Desktop/rstack/rslib-project/rslib-template/node_modules/.pnpm/@rsbuild+core@1.4.9/node_modules/@rsbuild/node_modules:/Users/zhangyi/Desktop/rstack/rslib-project/rslib-template/node_modules/.pnpm/@rsbuild+core@1.4.9/node_modules:/Users/zhangyi/Desktop/rstack/rslib-project/rslib-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhangyi/Desktop/rstack/rslib-project/rslib-template/node_modules/.pnpm/@rsbuild+core@1.4.9/node_modules/@rsbuild/core/bin/node_modules:/Users/zhangyi/Desktop/rstack/rslib-project/rslib-template/node_modules/.pnpm/@rsbuild+core@1.4.9/node_modules/@rsbuild/core/node_modules:/Users/zhangyi/Desktop/rstack/rslib-project/rslib-template/node_modules/.pnpm/@rsbuild+core@1.4.9/node_modules/@rsbuild/node_modules:/Users/zhangyi/Desktop/rstack/rslib-project/rslib-template/node_modules/.pnpm/@rsbuild+core@1.4.9/node_modules:/Users/zhangyi/Desktop/rstack/rslib-project/rslib-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@rsbuild+core@1.4.9/node_modules/@rsbuild/core/bin/rsbuild.js" "$@"
else
  exec node  "$basedir/../../../../../@rsbuild+core@1.4.9/node_modules/@rsbuild/core/bin/rsbuild.js" "$@"
fi
